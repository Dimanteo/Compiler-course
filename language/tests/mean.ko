mean(numbers[100]) {
    let sum = 0;
    let n = 0;
    while (n != 100) {
        sum = sum + numbers[n];
        n = n + 1;
    }
    return sum / n;
}

main() {
    let i = 0;
    let data[100];
    while (i != 100) {
        data[i] = i + 101;
        i = i + 1;
    }
    print(mean(data));
    return 0;
}