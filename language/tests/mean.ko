mean() {
    let numbers[100];
    let idx = 0;
    while (idx < 100) {
        numbers[idx] = 100 + idx + 1;
        idx = idx + 1;
    }
    let sum = 0;
    let n = 0;
    while (idx > 0) {
        idx = idx - 1;
        n = n + 1;
        sum = sum + numbers[idx];
        print(sum);
    }
    print(sum);
    print(n);
    return sum / n;
}

main() {
    print(mean());
    return 0;
}